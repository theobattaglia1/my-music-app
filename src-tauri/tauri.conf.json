{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:8080",
    "distDir": "../dist"
  },
  "package": {
    "productName": "My Music App",
    "version": "0.1.0"
  },
    "tauri": {
      "allowlist": {
        "all": false,
        "shell": {
          "all": false,
          "open": true
        },
        "dialog": {
          "all": true
        },
        "fs": {
          "all": true,
          "scope": ["**"]
        },
        "path": {
          "all": true
        },
        "protocol": {
          "asset": true,
          "assetScope": ["**"]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.mymusic.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
"windows": [
  {
    "title": "My Music App",
    "width": 1200,
    "height": 800,
    "resizable": true,
    "fullscreen": false,
    "visible": true,
    "center": true,
    "minWidth": 800,
    "minHeight": 600,
    "fileDropEnabled": false
  }
]
  }
}